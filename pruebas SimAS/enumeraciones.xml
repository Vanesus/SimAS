<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="gramatica.xsl"?>
<grammar>
	<name>Enumeraciones</name>
	<description>Gramatica que genera enumeraciones del lenguaje de programacion C</description>
	<non-terminal-symbols>
		<non-terminal>
			<value>S</value>
		</non-terminal>
		<non-terminal>
			<value>D</value>
		</non-terminal>
		<non-terminal>
			<value>L</value>
		</non-terminal>
	</non-terminal-symbols>
	<terminal-symbols>
		<terminal>
			<value>enum</value>
		</terminal>
		<terminal>
			<value>identificador</value>
		</terminal>
		<terminal>
			<value>{</value>
		</terminal>
		<terminal>
			<value>}</value>
		</terminal>
		<terminal>
			<value>;</value>
		</terminal>
		<terminal>
			<value>.</value>
		</terminal>
	</terminal-symbols>
	<init-symbol>S</init-symbol>
	<rule-set>
		<rule>
			<value>S → S D </value>
		</rule>
		<rule>
			<value>S → ε </value>
		</rule>
		<rule>
			<value>D → enum identificador { L } ; </value>
		</rule>
		<rule>
			<value>L → identificador </value>
		</rule>
		<rule>
			<value>L → L . identificador </value>
		</rule>
	</rule-set>
</grammar>
