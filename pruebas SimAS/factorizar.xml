<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="gramatica.xsl"?>
<grammar>
	<name>factorizar</name>
	<description>Gramatica para factorizar</description>
	<non-terminal-symbols>
		<non-terminal>
			<value>S</value>
		</non-terminal>
		<non-terminal>
			<value>A</value>
		</non-terminal>
		<non-terminal>
			<value>B</value>
		</non-terminal>
	</non-terminal-symbols>
	<terminal-symbols>
		<terminal>
			<value>a</value>
		</terminal>
		<terminal>
			<value>b</value>
		</terminal>
	</terminal-symbols>
	<init-symbol>S</init-symbol>
	<rule-set>
		<rule>
			<value>S → a B a </value>
		</rule>
		<rule>
			<value>S → b A b </value>
		</rule>
		<rule>
			<value>S → a a b </value>
		</rule>
		<rule>
			<value>S → b b A </value>
		</rule>
		<rule>
			<value>A → a A </value>
		</rule>
		<rule>
			<value>A → a </value>
		</rule>
		<rule>
			<value>B → b B </value>
		</rule>
		<rule>
			<value>B → b </value>
		</rule>
	</rule-set>
</grammar>
