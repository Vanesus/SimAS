<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="gramatica.xsl"?>
<grammar>
	<name>ejemplo2</name>
	<description>palindromo par</description>
	<non-terminal-symbols>
		<non-terminal>
			<value>S</value>
		</non-terminal>
		<non-terminal>
			<value>A</value>
		</non-terminal>
	</non-terminal-symbols>
	<terminal-symbols>
		<terminal>
			<value>a</value>
		</terminal>
		<terminal>
			<value>b</value>
		</terminal>
	</terminal-symbols>
	<init-symbol>S</init-symbol>
	<rule-set>
		<rule>
			<value>S → ε </value>
		</rule>
		<rule>
			<value>S → a S a </value>
		</rule>
		<rule>
			<value>S → a A a </value>
		</rule>
		<rule>
			<value>A → ε </value>
		</rule>
		<rule>
			<value>A → b A b </value>
		</rule>
	</rule-set>
</grammar>
