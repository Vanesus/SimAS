<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="gramatica.xsl"?>
<grammar>
	<name>ejemplo3</name>
	<description>ejemplo 3</description>
	<non-terminal-symbols>
		<non-terminal>
			<value>S</value>
		</non-terminal>
		<non-terminal>
			<value>E</value>
		</non-terminal>
		<non-terminal>
			<value>T</value>
		</non-terminal>
		<non-terminal>
			<value>F</value>
		</non-terminal>
	</non-terminal-symbols>
	<terminal-symbols>
		<terminal>
			<value>id</value>
		</terminal>
		<terminal>
			<value>=</value>
		</terminal>
		<terminal>
			<value>+</value>
		</terminal>
		<terminal>
			<value>*</value>
		</terminal>
		<terminal>
			<value>n</value>
		</terminal>
		<terminal>
			<value>(</value>
		</terminal>
		<terminal>
			<value>)</value>
		</terminal>
	</terminal-symbols>
	<init-symbol>S</init-symbol>
	<rule-set>
		<rule>
			<value>S → id = E </value>
		</rule>
		<rule>
			<value>E → T + E </value>
		</rule>
		<rule>
			<value>E → T </value>
		</rule>
		<rule>
			<value>T → F * T </value>
		</rule>
		<rule>
			<value>T → F </value>
		</rule>
		<rule>
			<value>F → id </value>
		</rule>
		<rule>
			<value>F → n </value>
		</rule>
		<rule>
			<value>F → ( E ) </value>
		</rule>
	</rule-set>
</grammar>
