<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="gramatica.xsl"?>
<grammar>
	<name>Expresiones</name>
	<description>Gramática que genera expresiones</description>
	<non-terminal-symbols>
		<non-terminal>
			<value>E</value>
		</non-terminal>
	</non-terminal-symbols>
	<terminal-symbols>
		<terminal>
			<value>+</value>
		</terminal>
		<terminal>
			<value>*</value>
		</terminal>
		<terminal>
			<value>(</value>
		</terminal>
		<terminal>
			<value>)</value>
		</terminal>
		<terminal>
			<value>ID</value>
		</terminal>
		<terminal>
			<value>NUM</value>
		</terminal>
	</terminal-symbols>
	<init-symbol>E</init-symbol>
	<rule-set>
		<rule>
			<value>E → E + E </value>
		</rule>
		<rule>
			<value>E → E * E </value>
		</rule>
		<rule>
			<value>E → ( E ) </value>
		</rule>
		<rule>
			<value>E → ID </value>
		</rule>
		<rule>
			<value>E → NUM </value>
		</rule>
	</rule-set>
</grammar>
