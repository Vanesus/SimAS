<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="gramatica.xsl"?>
<grammar>
	<name>LALR</name>
	<description>sdafk</description>
	<non-terminal-symbols>
		<non-terminal>
			<value>S</value>
		</non-terminal>
		<non-terminal>
			<value>A</value>
		</non-terminal>
		<non-terminal>
			<value>B</value>
		</non-terminal>
	</non-terminal-symbols>
	<terminal-symbols>
		<terminal>
			<value>a</value>
		</terminal>
		<terminal>
			<value>d</value>
		</terminal>
		<terminal>
			<value>e</value>
		</terminal>
		<terminal>
			<value>c</value>
		</terminal>
		<terminal>
			<value>b</value>
		</terminal>
	</terminal-symbols>
	<init-symbol>S</init-symbol>
	<rule-set>
		<rule>
			<value>S → a A d </value>
		</rule>
		<rule>
			<value>S → b B d </value>
		</rule>
		<rule>
			<value>S → a B e </value>
		</rule>
		<rule>
			<value>S → b A e </value>
		</rule>
		<rule>
			<value>A → c </value>
		</rule>
		<rule>
			<value>B → c </value>
		</rule>
	</rule-set>
</grammar>
