<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="gramatica.xsl"?>
<grammar>
	<name>Gramática no ambigua que genera expresiones</name>
	<description>Gramática no ambigua que genera expresiones</description>
	<non-terminal-symbols>
		<non-terminal>
			<value>E</value>
		</non-terminal>
		<non-terminal>
			<value>T</value>
		</non-terminal>
		<non-terminal>
			<value>F</value>
		</non-terminal>
	</non-terminal-symbols>
	<terminal-symbols>
		<terminal>
			<value>+</value>
		</terminal>
		<terminal>
			<value>*</value>
		</terminal>
		<terminal>
			<value>(</value>
		</terminal>
		<terminal>
			<value>)</value>
		</terminal>
		<terminal>
			<value>ID</value>
		</terminal>
		<terminal>
			<value>NUM</value>
		</terminal>
	</terminal-symbols>
	<init-symbol>E</init-symbol>
	<rule-set>
		<rule>
			<value>E → E + T </value>
		</rule>
		<rule>
			<value>E → T </value>
		</rule>
		<rule>
			<value>T → T * F </value>
		</rule>
		<rule>
			<value>T → F </value>
		</rule>
		<rule>
			<value>F → ( E ) </value>
		</rule>
		<rule>
			<value>F → ID </value>
		</rule>
		<rule>
			<value>F → NUM </value>
		</rule>
	</rule-set>
</grammar>
