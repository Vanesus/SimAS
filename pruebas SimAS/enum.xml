<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="gramatica.xsl"?>
<grammar>
	<name>jklñ</name>
	<description>jñkl</description>
	<non-terminal-symbols>
		<non-terminal>
			<value>S</value>
		</non-terminal>
		<non-terminal>
			<value>D</value>
		</non-terminal>
		<non-terminal>
			<value>L</value>
		</non-terminal>
	</non-terminal-symbols>
	<terminal-symbols>
		<terminal>
			<value>enum</value>
		</terminal>
		<terminal>
			<value>id</value>
		</terminal>
		<terminal>
			<value>{</value>
		</terminal>
		<terminal>
			<value>}</value>
		</terminal>
		<terminal>
			<value>,</value>
		</terminal>
		<terminal>
			<value>;</value>
		</terminal>
	</terminal-symbols>
	<init-symbol>S</init-symbol>
	<rule-set>
		<rule>
			<value>S → S D </value>
		</rule>
		<rule>
			<value>S → ε </value>
		</rule>
		<rule>
			<value>D → enum id { L } ; </value>
		</rule>
		<rule>
			<value>L → L , id </value>
		</rule>
		<rule>
			<value>L → id </value>
		</rule>
	</rule-set>
</grammar>
