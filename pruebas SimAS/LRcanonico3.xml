<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="gramatica.xsl"?>
<grammar>
	<name>LR-canonico3</name>
	<description>fdsa</description>
	<non-terminal-symbols>
		<non-terminal>
			<value>S</value>
		</non-terminal>
		<non-terminal>
			<value>L</value>
		</non-terminal>
		<non-terminal>
			<value>R</value>
		</non-terminal>
	</non-terminal-symbols>
	<terminal-symbols>
		<terminal>
			<value>=</value>
		</terminal>
		<terminal>
			<value>*</value>
		</terminal>
		<terminal>
			<value>id</value>
		</terminal>
	</terminal-symbols>
	<init-symbol>S</init-symbol>
	<rule-set>
		<rule>
			<value>S → L = R </value>
		</rule>
		<rule>
			<value>S → R </value>
		</rule>
		<rule>
			<value>L → * R </value>
		</rule>
		<rule>
			<value>L → id </value>
		</rule>
		<rule>
			<value>R → L </value>
		</rule>
	</rule-set>
</grammar>
