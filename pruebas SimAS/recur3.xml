<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="gramatica.xsl"?>
<grammar>
	<name>recur3</name>
	<description>recur3</description>
	<non-terminal-symbols>
		<non-terminal>
			<value>E</value>
		</non-terminal>
	</non-terminal-symbols>
	<terminal-symbols>
		<terminal>
			<value>var</value>
		</terminal>
		<terminal>
			<value>num</value>
		</terminal>
		<terminal>
			<value>+</value>
		</terminal>
		<terminal>
			<value>*</value>
		</terminal>
	</terminal-symbols>
	<init-symbol>E</init-symbol>
	<rule-set>
		<rule>
			<value>E → E + E </value>
		</rule>
		<rule>
			<value>E → E * E </value>
		</rule>
		<rule>
			<value>E → var </value>
		</rule>
		<rule>
			<value>E → num </value>
		</rule>
	</rule-set>
</grammar>
