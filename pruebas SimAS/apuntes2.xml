<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="gramatica.xsl"?>
<grammar>
	<name>apuntes 2</name>
	<description>Ejemplo de los apuntes</description>
	<non-terminal-symbols>
		<non-terminal>
			<value>S</value>
		</non-terminal>
		<non-terminal>
			<value>S'</value>
		</non-terminal>
		<non-terminal>
			<value>D</value>
		</non-terminal>
		<non-terminal>
			<value>T</value>
		</non-terminal>
		<non-terminal>
			<value>L</value>
		</non-terminal>
		<non-terminal>
			<value>L'</value>
		</non-terminal>
	</non-terminal-symbols>
	<terminal-symbols>
		<terminal>
			<value>;</value>
		</terminal>
		<terminal>
			<value>int</value>
		</terminal>
		<terminal>
			<value>float</value>
		</terminal>
		<terminal>
			<value>id</value>
		</terminal>
		<terminal>
			<value>,</value>
		</terminal>
	</terminal-symbols>
	<init-symbol>S</init-symbol>
	<rule-set>
		<rule>
			<value>S → D S' </value>
		</rule>
		<rule>
			<value>S' → D S' </value>
		</rule>
		<rule>
			<value>S' → ε </value>
		</rule>
		<rule>
			<value>D → T L ; </value>
		</rule>
		<rule>
			<value>T → int </value>
		</rule>
		<rule>
			<value>T → float </value>
		</rule>
		<rule>
			<value>L → id , L' </value>
		</rule>
		<rule>
			<value>L' → , id L' </value>
		</rule>
		<rule>
			<value>L' → ε </value>
		</rule>
	</rule-set>
</grammar>
