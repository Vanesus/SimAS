<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="gramatica.xsl"?>
<grammar>
	<name>ejemplo4</name>
	<description>sadjkf</description>
	<non-terminal-symbols>
		<non-terminal>
			<value>S</value>
		</non-terminal>
		<non-terminal>
			<value>D</value>
		</non-terminal>
		<non-terminal>
			<value>T</value>
		</non-terminal>
		<non-terminal>
			<value>P</value>
		</non-terminal>
		<non-terminal>
			<value>L</value>
		</non-terminal>
	</non-terminal-symbols>
	<terminal-symbols>
		<terminal>
			<value>id</value>
		</terminal>
		<terminal>
			<value>(</value>
		</terminal>
		<terminal>
			<value>)</value>
		</terminal>
		<terminal>
			<value>;</value>
		</terminal>
		<terminal>
			<value>int</value>
		</terminal>
		<terminal>
			<value>,</value>
		</terminal>
	</terminal-symbols>
	<init-symbol>S</init-symbol>
	<rule-set>
		<rule>
			<value>S → D S </value>
		</rule>
		<rule>
			<value>S → ε </value>
		</rule>
		<rule>
			<value>D → T id ( P ) ; </value>
		</rule>
		<rule>
			<value>T → int </value>
		</rule>
		<rule>
			<value>P → ε </value>
		</rule>
		<rule>
			<value>P → T L </value>
		</rule>
		<rule>
			<value>L → , T L </value>
		</rule>
		<rule>
			<value>L → ε </value>
		</rule>
	</rule-set>
</grammar>
