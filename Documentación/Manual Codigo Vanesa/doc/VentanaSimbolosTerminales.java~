  private  DefaultListModel modeloListaTerminales ;
  private  DefaultListModel modeloListaNoTerminales ;
  private  PanelCreacionGramaticaPaso2 panelPadre ;

  
  public VentanaSimbolosTerminales(ListModel simbolosTerminales,  ListModel simbolosNoTerminales,  PanelCreacionGramaticaPaso2 ventanaPadre) {
 
     int i= 0;
     
     DefaultListModel list = new DefaultListModel();
     this.modeloListaTerminales =list;
     i=0;
     while(true){
        if(i >= simbolosTerminales.getSize()) {
            break;
        }
        if(i < simbolosTerminales.getSize())  {
            this.modeloListaTerminales.add(i,simbolosTerminales.getElementAt(i));
            i = i+1;    
        }
    }
    DefaultListModel lista = new DefaultListModel();
    this.modeloListaNoTerminales =lista;
    i=0;
    while(true) {
      if(i >= simbolosNoTerminales.getSize()) {
        break;
      }
      if(i < simbolosNoTerminales.getSize())  {
        this.modeloListaNoTerminales.add(i,simbolosNoTerminales.getElementAt(i));
        i = i+1;    
      }
    }
    
    initComponents();
    this.jList1.setModel(simbolosTerminales);

    this.setResizable(false);
    this.panelPadre =ventanaPadre;
    this.jTextField1.setText("");
   // this.btnTerminal13.setText("\u03b5");
  //  this.btnTerminal13.setToolTipText("<html>Inserta el s\u00edmbolo <b>\u03b5</b>.</html>");
   // this.txtSimboloTerminal.requestFocus();
  //  this.estaVentana =this;
   /* VentanaSimbolosTerminales prueba = new VentanaSimbolosTerminales(this, null, null);
    this.jTextField1.addKeyListener(prueba);
    VentanaGestionarSimbolosTerminales$4 JdecGenerated190 = new VentanaGestionarSimbolosTerminales$4(this);
    this.txtSimboloTerminal.addActionListener(JdecGenerated190);
    return;*/
 }
