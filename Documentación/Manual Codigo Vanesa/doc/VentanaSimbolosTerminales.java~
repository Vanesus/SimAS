    private void jButtonCancelarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButtonCancelarActionPerformed
        int conf = JOptionPane.showConfirmDialog(null, "¿Desea salir de la edición de la gramática?", "Salir",JOptionPane.YES_NO_OPTION);

        if(conf==0)
            this.dispose();
    }//GEN-LAST:event_jButtonCancelarActionPerformed

    private void jButtonInsertarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButtonInsertarActionPerformed
     /*   DefaultListModel listModel = new DefaultListModel();
        String elemento = jTextField1.getText();
        listModel.addElement(elemento);*/
        String elemento = jTextField1.getText();
        int encontrado= 0;
        int i= 0;
        
        if(elemento.equals("")){
            encontrado =1;
        }else{
        while(true)    {
            if(i >= this.modeloListaTerminales.getSize())  {
                break;
            }
            if(i < this.modeloListaTerminales.getSize()) {
                if(this.modeloListaTerminales.getElementAt(i).equals(elemento)) {
                    encontrado=1;
                }
                 
            i = i+1;
        //continue  ;
            }
      }
   
    }
    if(encontrado==0)
    {
      this.modeloListaTerminales.add(this.modeloListaTerminales.getSize(),elemento);
      this.jList1.setModel(this.modeloListaTerminales);
   
    }
    this.jTextField1.requestFocus();
    this.jTextField1.setText("");
    }//GEN-LAST:event_jButtonInsertarActionPerformed

    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1ActionPerformed
        int encontrado= 0;
        int i= 0;
        encontrado=0;
        i=0;
        while(true)  {
            if(i >= this.modeloListaTerminales.getSize()) {
                break;
            }
            if(i < this.modeloListaTerminales.getSize()) {
                if(this.modeloListaTerminales.getElementAt(i).equals("+")) {
                    encontrado=1;
                }
            i = i+1;    
            }
        }
        if(encontrado==0) {
            this.modeloListaTerminales.add(this.modeloListaTerminales.getSize(),"+");
            this.jList1.setModel(this.modeloListaTerminales);
        }
        this.jTextField1.requestFocus();
    }//GEN-LAST:event_jButton1ActionPerformed

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton2ActionPerformed
        int encontrado= 0;
        int i= 0;
        while(true)  {
            if(i >= this.modeloListaTerminales.getSize())  {
                break;
            }
            if(i < this.modeloListaTerminales.getSize()) {
                if(this.modeloListaTerminales.getElementAt(i).equals("-")) {
                    encontrado=1;
                }
            i = i+1;    
            }
        }
        if(encontrado==0) {
            this.modeloListaTerminales.add(this.modeloListaTerminales.getSize(),"-");
            this.jList1.setModel(this.modeloListaTerminales);
        }
        this.jTextField1.requestFocus();
    }//GEN-LAST:event_jButton2ActionPerformed

    private void jButtonAceptarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButtonAceptarActionPerformed
        if(this.panelPadre!=  null)  {
            if(this.jList1.getModel().getSize() > 0)  { 
                this.panelPadre.asignarListaSimbolosTerminales((DefaultListModel)this.jList1.getModel());
             }
       }
        this.dispose();
    }//GEN-LAST:event_jButtonAceptarActionPerformed

    private void jButtonEliminarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButtonEliminarActionPerformed
        DefaultListModel modelo= null;
        DefaultListModel producciones = this.panelPadre.getProducciones();                
        String[] pr;
        ArrayList<String> prod = new ArrayList();
        String nt;
        int seleccion= 0;
        nt = this.jList1.getSelectedValue().toString();
        
        
        seleccion=this.jList1.getSelectedIndex();
        if(seleccion != -1) {
            if(producciones != null){
                int i=0;                
                while(i< producciones.size()){
                    int j=0;
                    pr = (producciones.getElementAt(i).toString()).split(" ");                    
                    while(j < pr.length){
                        if(pr[j].equals(this.jList1.getSelectedValue())){                            
                            prod.add(producciones.getElementAt(i).toString());                                                                                            
                        }
                        j++;
                    }
                    i++;
                }
            }
            if(prod.size()>0){                
                int conf = JOptionPane.showConfirmDialog(null, "El símbolo "+nt+" ha sido usado, si lo elimina también se eliminarán las producciones en las que aparezca, ¿desea eliminarlo?", "Símbolo usado",JOptionPane.YES_NO_OPTION);
                if(conf==0){                    
                    int k=0;
                    while(k < prod.size()){                        
                        int l=0;
                        while(l < producciones.size()){
                            if(prod.get(k).equals(producciones.get(l).toString())){
                                producciones.remove(l);
                                break;
                            }else
                                l++;
                        }                        
                        k++;
                    }
                    
                    this.panelPadre.setProducciones(producciones);
                    modelo=(DefaultListModel)this.jList1.getModel();
                    modelo.remove(seleccion);                     
                }
                
            }else{
                modelo=(DefaultListModel)this.jList1.getModel();
                modelo.remove(seleccion);   
            }
        }
        
        
        
        /*DefaultListModel modelo= null;
        int seleccion= 0;
        seleccion=this.jList1.getSelectedIndex();
        
        if(seleccion != -1)  {
            modelo=(DefaultListModel)this.jList1.getModel();
            modelo.remove(seleccion);
        }*/
    }//GEN-LAST:event_jButtonEliminarActionPerformed

    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton3ActionPerformed
        int encontrado= 0;
        int i= 0;

        while(true) {
            if(i >= this.modeloListaTerminales.getSize()) {
                break;
            }
            if(i < this.modeloListaTerminales.getSize())  {
                if(this.modeloListaTerminales.getElementAt(i).equals("*")) {
                    encontrado=1;
                }
            i = i+1;
            }
        }
        if(encontrado==0){
            this.modeloListaTerminales.add(this.modeloListaTerminales.getSize(),"*");
            this.jList1.setModel(this.modeloListaTerminales);
        }
        this.jTextField1.requestFocus();
    }//GEN-LAST:event_jButton3ActionPerformed

    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton4ActionPerformed
        int encontrado= 0;
        int i= 0;

        while(true) {
            if(i >= this.modeloListaTerminales.getSize()){
                break;
            }
            if(i < this.modeloListaTerminales.getSize()) {
                if(this.modeloListaTerminales.getElementAt(i).equals("/")) {
                    encontrado=1;
                }
            i = i+1;
            }
        }
        if(encontrado==0) {
            this.modeloListaTerminales.add(this.modeloListaTerminales.getSize(),"/");
            this.jList1.setModel(this.modeloListaTerminales);
        }
        this.jTextField1.requestFocus();
    }//GEN-LAST:event_jButton4ActionPerformed

    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton5ActionPerformed
        int encontrado= 0;
        int i= 0;
        
        while(true) {
            if(i >= this.modeloListaTerminales.getSize()){
                break;
            }
            if(i < this.modeloListaTerminales.getSize()) {
                if(this.modeloListaTerminales.getElementAt(i).equals("(")) {
                    encontrado=1;
                }
            i = i+1;
            }
        }
        if(encontrado==0) {
            this.modeloListaTerminales.add(this.modeloListaTerminales.getSize(),"(");
            this.jList1.setModel(this.modeloListaTerminales);   
        }
        this.jTextField1.requestFocus();
    }//GEN-LAST:event_jButton5ActionPerformed

